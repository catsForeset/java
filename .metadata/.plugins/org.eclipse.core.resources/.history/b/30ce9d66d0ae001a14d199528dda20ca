package mytest;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 10;
	      while( x < 20 ) {
	         System.out.print("value of x : " + x );
	         x++;
	         System.out.print("\n");
	      }
	      //先执行一次，再判断条件
	      do {
	    	 System.out.print("value of in do while x : " + x );
	         x++;
	         System.out.print("\n");
	      }while(x < 20);
	}

}
